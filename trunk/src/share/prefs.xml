<?xml version="1.0" encoding="UTF-8"?>
<settings>
    <default_database>QMYSQL</default_database>
    <editor>
        <font_name>Monospace</font_name>
        <font_size>10</font_size>
    </editor>
    <highlighting>
        <groupname>sql_basics</groupname>
        <color>#000000</color>
        <bold>true</bold>
    </highlighting>
    <highlighting>
        <groupname>sql_functions</groupname>
        <color>#8c658a</color>
    </highlighting>
    <highlighting>
        <groupname>sql_types</groupname>
        <color>#005b90</color>
    </highlighting>
    <highlighting>
        <groupname>strings</groupname>
        <color>#c30000</color>
    </highlighting>
    <highlighting>
        <groupname>numbers</groupname>
        <color>#b39115</color>
    </highlighting>
    <highlighting>
        <groupname>comments</groupname>
        <color>#7f7f7f</color>
        <italic>true</italic>
    </highlighting>
</settings>
